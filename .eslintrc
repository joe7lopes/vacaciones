{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "comma-dangle": 0,
      "react/jsx-filename-extension": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "class-methods-use-this": "off",
      "no-use-before-define": "off",
      "react/prop-types": "warn",
      "react/no-did-mount-set-state": "warn"
    }
  }